.section .data

emergenza:
	.ascii "Modalit√† controllo emergenza inserita\n"
emergenza_len:
	.long . - emergenza

.section .text

.global controllo_emergenza

.type controllo_emergenza, @function

controllo_emergenza:
	movl $4, %eax
	movl $1, %ebx
	leal emergenza, %ecx
	movl emergenza_len, %edx
	int $0x80

	ret
